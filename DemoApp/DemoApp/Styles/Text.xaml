<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary x:Class="DemoApp.Styles.Text"
                    xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <x:Double x:Key="TextSizeIcon">29</x:Double>
    <x:Double x:Key="TextSizeHeadline">22</x:Double>
    <x:Double x:Key="TextSizeTitle">16</x:Double>
    <x:Double x:Key="TextSizeBody">14</x:Double>
    <x:Double x:Key="TextSizeTiny">13</x:Double>

    <OnPlatform x:Key="FontAwesome"
                x:TypeArguments="x:String"
                Android="fa-solid-900.ttf#Font Awesome 5 Free"
                iOS="FontAwesome5Free-Solid" />


</ResourceDictionary>